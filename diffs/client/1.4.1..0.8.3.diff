+              type="radio"
+              value={get(values, 'gender')}
+              error={get(touched, 'gender') && get(errors, 'gender')}
+              onChange={(value) => setFieldValue('gender', value)}
+            >
+              {genderList.map((gender) => [
+                <RadioButton
+                  key={gender}
+                  value={gender}
