+import { FlexedScrollView, EventInfoItem } from './components';
+
+const handlePressAnchor = (url) => {
+  InAppBrowser.open(url);
+};
+
+const Event = ({ event, loading }) => {
+  const coverImageSources = get(event, 'image.sources', []);
