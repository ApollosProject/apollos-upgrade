+
+const changeLivestream = ({ isLive }) =>
+  client.writeQuery({
+    query: GET_LIVE_STREAM,
+    data: {
+      liveStream: {
+        __typename: 'LiveStream',
+        isLive,
+      },
+    },
+  });
+
+const ChangeLivestream = () => (
+  <Query query={GET_LIVE_STREAM}>
+    {({ data }) => {
+      const isLive = get(data, 'liveStream.isLive', false);
