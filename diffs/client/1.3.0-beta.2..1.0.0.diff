+    variables={{ first: 3 }}
+  >
+    {({
+      loading,
+      data: { likedContent: { edges = [] } = { edges: [] } } = {},
+    }) => {
+      if (!edges.length) return null;
+      return (
+        <RecentlyLikedTileFeed
+          id={'liked'}
+          name={'Recently Liked'}
+          content={edges.map((e) => e.node)}
+          isLoading={loading}
+          navigation={navigation}
+          loadingStateObject={{
+            title: 'Recently Liked',
+            isLoading: true,
+          }}
+        />
+      );
