+          }}
+        />
+      );
+    }}
+  </Query>
+);
+
+RecentlyLikedTileFeedConnected.propTypes = {
+  navigation: PropTypes.shape({
+    getParam: PropTypes.func,
+    navigate: PropTypes.func,
+  }),
+};
+
+export default RecentlyLikedTileFeedConnected;
