+} from 'apollo-cache-inmemory';
+import introspectionQueryResultData from '../fragmentTypes.json';
+
+const nodeCacheRedirect = (_, { id }, { getCacheKey }) =>
+  id ? getCacheKey({ __typename: id.split(':')[0], id }) : null;
