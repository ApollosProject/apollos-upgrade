+  Authentication: {
+    user: (root, args, { dataSources }) => dataSources.Auth.getCurrentPerson(),
+  },
+  Mutation: {
+    authenticate: (root, { identity, password }, { dataSources }) =>
+      dataSources.Auth.authenticate({ identity, password }),
