 import ApollosConfig from '@apollosproject/config';
 
 ApollosConfig.loadYaml({
-  configPath: path.join(__dirname, '..', 'config.yml'),
+  configPath: path.join(
+    __dirname,
+    '..',
+    fs.existsSync(path.join(__dirname, '..', 'config.postgres.yml'))
